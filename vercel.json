{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "out"
  }